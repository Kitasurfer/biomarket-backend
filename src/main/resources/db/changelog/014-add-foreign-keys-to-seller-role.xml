<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="014-add-foreign-keys-to-seller-role" author="Kristina">

        <addForeignKeyConstraint baseTableName="seller_role"
                                 baseColumnNames="seller_id"
                                 constraintName="fk_seller_role_to_seller"
                                 referencedTableName="seller"
                                 referencedColumnNames="id"/>

        <addForeignKeyConstraint baseTableName="seller_role"
                                 baseColumnNames="role_id"
                                 constraintName="fk_seller_role_to_role"
                                 referencedTableName="role"
                                 referencedColumnNames="id"/>

    </changeSet>

</databaseChangeLog>
